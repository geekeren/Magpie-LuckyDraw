    "url": "https://github.com/geekeren/Magpie-LuckyDraw.git"
  },
  "bugs": {
    "url": "https://github.com/geekeren/Magpie-LuckyDraw/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.1.0",
    "electron-updater": "^4.0.6",
    "fireworks-react": "^1.0.12",
    "gh-pages": "^2.0.1",
    "jquery": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-edit-inline": "^1.0.8",
    "react-particles-js": "^2.6.0",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-sortable-hoc": "^1.9.1",
    "react-table": "^6.10.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.40.2",
    "react-scripts": "3.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "release-it": "^12.2.1",
    "typescript": "^3.4.5",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron-start": "electron .",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-pack": "build -c.extraMetadata.main=build/electron.js",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "pre-deploy": "yarn build",
    "deploy": "yarn deploy:app && yarn deploy:web",
    "deploy:app": "rm -rf dist && electron-builder -mwl",
    "deploy:web": "gh-pages -d build",
    "release": "release-it"
  },
  "bin": {
    "magpie": "./cli.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
